{
  "family": "filtering-service-task",
  "containerDefinitions": [
    {
      "image": "docker.io/danieltnaves/filtering-service:%BUILD_NUMBER%",
      "name": "filtering-service-container",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 80
        }
      ]
    }
  ]
}
